unit Co1py4Unit;

interface

uses
  Winapi.Windows, AddreUnit, Me2m4o1ryUnit; // NoReload,HeadShot;

procedure CshellCopySet;

implementation

var
 cshellCopy: Array [1111111 .. $15FFFFF] of Byte;

 procedure CshellCopySet;
var
  Index: Integer;
begin
  ModuleCopyAdr := dword(VirtualAllocEx(hProcess, nil, $15FFFFF, MEM_COMMIT,
    PAGE_EXECUTE_READWRITE));
  ReadProcessMemory(hProcess, Ptr(cshell.addr + $A00000), @cshellCopy, $700000,
    PNativeUint(Nil)^);
  WriteProcessMemory(hProcess, Ptr(ModuleCopyAdr + $A00000), @cshellCopy,
    $700000, PNativeUint(Nil)^);

  C0py4d41 := Rpm4byte(cshell.addr + English + $76C);
  C0py4d42 := Rpm4byte(Rpm4byte(C0py4d41 + $14));
  C0py4d43 := C0py4d42 - cshell.addr;

  C0py4d44 := Rpm4byte(cshell.addr + English + $76C);
  C0py4d45 := Rpm4byte(C0py4d44 + $14);

  FillChar(cshellCopy, SizeOf(cshellCopy), 0);
end;

end.

<mapper namespace="boardMapper">

	<resultMap type="edu.kh.fin.board.model.vo.Board" id="board_rm">
	
		<id property="boardNo" 			column="BOARD_NO"/>
		
	</resultMap>
	
	<!-- 전체 게시글 수 count -->
	<select id="getListCount">
		SELECT COUNT(*) FROM BOARD WHERE STATUS_CD != 4
	</select>
</mapper>

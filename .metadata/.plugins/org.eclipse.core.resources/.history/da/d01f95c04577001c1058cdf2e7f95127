package edu.kh.fin.chat.model.service;

import java.util.List;

import edu.kh.fin.chat.model.vo.ChatRoom;

public interface ChatService {

	/** 채팅방 목록 조회
	 * @return chatRoomList
	 */
	List<ChatRoom> chatRoomList();

	/** 채팅방 열기
	 * @param room
	 * @return chatRoomNo
	 */
	int openChatRoom(ChatRoom room);

}
